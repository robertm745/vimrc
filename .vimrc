set nocompatible
set nostartofline
syntax on
colorscheme desert
set tabstop=4
set expandtab
set number
set relativenumber
set showcmd
filetype plugin indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch
nnoremap <leader><space> :nohlsearch<CR>
nnoremap j gj
nnoremap k gk
set hidden
set smartcase
set ignorecase
set autoindent
set smartindent
set visualbell
set t_vb=
set mouse+=a
map Y y$
set laststatus=2
